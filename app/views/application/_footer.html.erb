    <div class="footer-wrapper">

        <!-- Products -->
        <ul class="footer-column">
            <li><h2>Products</h2></li>
            <li><%= link_to 'T-Shirts',   products_path(category: 'shirts') %></li>
            <li><%= link_to 'Hoodies',    products_path(category: 'hoodies') %></li>
            <li><%= link_to 'Trackies',   products_path(category: 'trackies') %></li>
            <li><%= link_to 'Shoes',      products_path(category: 'shoes') %></li>
            <li><%= link_to 'Browse all', products_path %></li>
        </ul>

        <!-- Site Information -->
        <ul class="footer-column">
            <li><h2>Luke Jamie</h2></li>
            <li><%= link_to 'About us', 'javascript:void(0)' %></li>
            <li><%= link_to 'Press kit', 'javascript:void(0)' %></li>
            <li><%= link_to 'Feedback', 'javascript:void(0)' %></li>
            <li><%= link_to 'Contact us', 'javascript:void(0)' %></li>
        </ul>

        <!-- Help & Support -->
        <ul class="footer-column">
            <li><h2>Support</h2></li>
            <li><%= link_to 'Help', support_articles_path %></li>
            <li><%= link_to 'Sizing chart', 'javascript:void(0)' %></li>
            <li><%= link_to 'Returns policy', 'javascript:void(0)' %></li>
            <li><%= link_to 'Terms of use', 'javascript:void(0)' %></li>
            <li><%= link_to 'Privacy policy', support_article_path('privacy-policy') %></li>
            <li><%= link_to 'Security', 'javascript:void(0)' %></li>
        </ul>

        <!-- Account info -->
        <ul class="footer-column">
            <li><h2>Account</h2></li>
            <% if user_signed_in? %>
                <li><%= link_to 'Overview', account_root_path %></li>
                <li><%= link_to 'Wishlist', 'javascript:void(0)' %></li>
                <li><%= link_to 'Addresses', account_addresses_path %></li>
                <li><%= link_to 'Order history', account_orders_path %></li>
                <li><%= link_to 'Settings', account_settings_path %></li>
                <li><%= link_to 'Logout', destroy_user_session_path, method: :delete, data: { confirm: 'Are you sure you want to log out?' } %></li>
            <% else %>
                <li><%= link_to 'Login', new_user_session_path %></li>
                <li><%= link_to 'Register', new_user_registration_path %></li>
                <li><%= link_to 'Reset password', new_user_password_path %></li>
            <% end %>
        </ul>

        <!-- Payment methods -->
        <ul class="footer-column" id="payment-methods">
            <li><h2>Payment methods</h2></li>
            <li>
                <!-- PayPal -->
                <%= link_to 'https://www.paypal.com/', target: '_blank' do %>
                    <i class="pw pw-paypal" title="PayPal"></i>
                <% end %>

                <!-- Mastercard -->
                <%= link_to 'https://www.mastercard.co.uk/content/mccom/en-gb.html/', target: '_blank' do %>
                    <i class="pw pw-mastercard" title="Mastercard"></i>
                <% end %>

                <!-- Visa -->
                <%= link_to 'https://www.visa.co.uk/', target: '_blank' do %>
                    <i class="pw pw-visa" title="Visa"></i>
                <% end %>

                <% if false -%>
                    <!-- American Express -->
                    <%= link_to 'https://www.americanexpress.com/uk/?location=globalsplash', target: '_blank' do %>
                        <i class="pw pw-american-express" title="American Express"></i>
                    <% end %>
                <% end %>
            </li>
        </ul>

    </div>

    <div class="footer-base">
        <div class="footer-wrapper">
            <%= link_to root_path do %>
                <%= image_tag 'luke-jamie.svg' %>
            <% end %>
            <p>Luke Jamie Â© <% '2017-' if Time.zone.now.year != 2017 %><%= Time.zone.now.year %></p>
        </div>
    </div>

</div>